    "nodemon": "/node_modules/nodemon/bin/nodemon",
    "ts-node": "ts-node",
    "test:watch": "mocha --register ts-node/register test/*.spec.ts  --watch --watch-extensions ts -r",
    "test": "mocha --register ts-node/register test/**/*.ts",
    "batch": "ts-node batch/batch.ts"
  },
